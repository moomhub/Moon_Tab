{
  "name": "moon-tab",
  "private": true,
  "version": "1.0.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && copy manifest.json dist\\",
    "build:windows": "vue-tsc --noEmit && vite build && copy manifest.json dist\\ && powershell Compress-Archive -Path dist -DestinationPath dist.zip -Force",
    "build:linux": "vue-tsc --noEmit && vite build && cp manifest.json dist/ && zip -r dist.zip dist",
    "preview": "vite preview"
  },
