    "dayjs": "^1.11.13",
    "file-saver": "^2.0.5",
    "gridstack": "^12.1.2",
    "html2canvas": "^1.4.1",
    "jszip": "^3.10.1",
    "localforage": "^1.10.0",
    "murmurhash": "^2.0.1",
    "nanoid": "^5.1.5",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "swiper": "^11.2.8",
    "tdesign-icons-vue-next": "^0.3.6",
    "tdesign-vue-next": "^1.13.0",
